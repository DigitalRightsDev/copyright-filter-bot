    <div class="social">
      <a href="<%= messages.twit %>"><i class="typcn typcn-social-twitter"></i></a>
      <div style="clear:both"></div>
        <div id="footer-logo">
        <a target="_blank" href="https://twitter.com/digitalrightsdv/"><img src="/logos/digitalrightsdev.png"/></a>
        <a href="/<%= messages.lang %>"><%= messages.about %></a> & <a href="https://github.com/DigitalRightsDev/copyright-filter-bot/" target="_blank"  class="typcn typcn-social-github"><i></i></a>
      </div>
    </div>
  </div>
</section>
  <script type="text/javascript" src="/assets/js/mobile-check.js"></script>
  </body>
</html>
