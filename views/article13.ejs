    <div class="message bot">
      <%= messages.message9 %>
    </div>
    <div class="message bot">
      <%= messages.message10 %>
    </div>
    <div class="message me">
      <%= messages.message11 %>
    </div>
    <div class="message me">
      <%= messages.message12 %>
    </div>
    <div class="message bot">
      <%= messages.message13 %>
    </div>
    <div class="message bot">
      <%= messages.message14 %>
    </div>
    <div class="message bot">
      <%= messages.message15 %>
    </div>
