    </div>
    <div class="meme">
      <img src="/assets/img/tell-me-again-meme-eu.gif">
    </div>
    <div class="message bot">
      <%= messages.message6 %>
    </div>
    <div class="message me">
      <%= messages.message7 %>
      <span id='computer'><i class='typcn typcn-times'></i>
        <%= messages.computer %>
      </span>
      <%= messages.it %>
    </div>
    <div class="message bot">
      <%= messages.quote %>
    </div>
    <div class="message me">
      <%= messages.sentences %>
    </div>
    <div class="meme">
      <img src="/assets/img/is-this-a-meme-eu.gif">
